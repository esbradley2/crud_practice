<h1>Edit your favorite</h1>

<%= form_tag("/favorites/#{@favorite.id }", :method => :patch) do %>
  <div>
    <%= label_tag :name %>
    <%= text_field_tag :name, @favorite.name %>
  </div>
  <div>
    <%= label_tag :url %>
    <%= text_field_tag :url, @favorite.url %>
  </div>
  <%= submit_tag :submit %>
<% end %>
